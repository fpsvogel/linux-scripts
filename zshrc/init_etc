# The kitchen sink. For miscellaneous things like initializing processes
# (other than Zsh plugins), adding to PATH, etc.

export PATH="$HOME/.rbenv/bin:$PATH"
eval "$(rbenv init -)"

[ -f "/home/felipe/.ghcup/env" ] && . "/home/felipe/.ghcup/env" # ghcup-env

export EDITOR=code

export PATH="$HOME/.bin:$PATH"

export CDPATH="$HOME:$HOME/code"
