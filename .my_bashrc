# My bash customizations.
# To use, add this line to ~/.bashrc:
#   source ~/<relative path>/.my_bashrc

# Easy-to-use variables containing ANSI colors.
source ~/scripts/.bashrc/ansi_colors

source ~/scripts/.bashrc/show_git_branch_in_prompt

source ~/scripts/.bashrc/aliases

export PATH="$HOME/.rbenv/bin:$PATH"
eval "$(rbenv init -)"
